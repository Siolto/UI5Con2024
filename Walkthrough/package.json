{
    "name": "UI5Con2024",
    "version": "1.0.0",
    "author": "Siolto",
    "description": "Let's test UI5 in 2024",
    "private": true,
    "scripts": {
        "start": "ui5 serve",
        "start:mock": "ui5 serve --config ui5-mock.yaml",
        "build": "ui5 build --all --clean-dest",
        "testrunner": "ui5-test-runner --coverage --url http://localhost:8080/test/testsuite.qunit.html",
        "qunit-service": "wdio run ./webapp/test/wdio.conf.js",
        "serve-dist": "ws --compress -d dist --open",
        "wdi5": "wdio run ./webapp/test/wdio.conf.js --suite wdi5"
    },
    "devDependencies": {
        "@openui5/types": "^1.124.0",
        "@sap-ux/ui5-middleware-fe-mockserver": "^2.2.42",
        "@sapui5/types": "^1.124.0",
        "@types/openui5": "^1.124.0",
        "@ui5/cli": "^3.10.3",
        "@ui5/ts-interface-generator": "^0.8.2",
        "@wdio/cli": "^8.38.2",
        "@wdio/local-runner": "^8.38.2",
        "@wdio/mocha-framework": "^8.38.2",
        "@wdio/spec-reporter": "^8.38.2",
        "@wdio/types": "^8.37.0",
        "local-web-server": "^5.3.3",
        "testrunner": "^1.0.4",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5",
        "ui5-middleware-serveframework": "^3.1.1",
        "ui5-middleware-simpleproxy": "^3.2.15",
        "ui5-test-runner": "^5.0.0",
        "ui5-tooling-transpile": "^3.4.1",
        "wdio-qunit-service": "^0.1.1",
        "wdio-ui5-service": "^2.0.9"
    }
}
